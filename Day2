#include <stdio.h>

int main() {
    int asciiValue;
    printf("Enter ASCII value: ");
    scanf("%d", &asciiValue);

    printf("The character for ASCII value %d is: %c\n", asciiValue, asciiValue);
    return 0;
}
#include<stdio.h>
int main(){
        int ASCIIvalue;
        scanf("%d",& ASCIIvalue);
        printf("%d %c\n",ASCIIvalue,ASCIIvalue);
        return 0;
}
#include<stdio.h>
int main(){
        int a,b;
        scanf("%d %d",&a,&b);
        printf("%d \t%d\n",a,b);
        return 0;
}
#include<stdio.h>
int main(){
        int a1=20,b1=40;
        printf("%d\n%d",a1,b1);
        return 0;
}
#include <stdio.h>
int main() {
    printf("Size of char: %zu byte\n", sizeof(char));
    printf("Size of int: %zu bytes\n", sizeof(int));
    printf("Size of float: %zu bytes\n", sizeof(float));
    printf("Size of double: %zu bytes\n", sizeof(double));
    return 0;
}
#include<stdio.h>
int main(){
        int m1=90;
        int m2=95;
        int m3=80;
        int m4=80;
        int m5=70;
        printf("%d\n",m1);
        printf("%d\n",m2);
        printf("%d\n",m3);
        printf("%d\n",m4);
        printf("%d\n",m5);
        return 0;
}
